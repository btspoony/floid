{
  "name": "floid",
  "version": "1.0.0",
  "description": "a DID Protocol on Flow blockchain",
  "engines": {
    "node": ">= 14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlowFans/floid.git"
  },
  "keywords": [
    "Flow",
    "Cadence",
    "DID"
  ],
  "author": "Tang BoHao<tech@btang.cn>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlowFans/floid/issues"
  },
  "homepage": "https://github.com/FlowFans/floid#readme",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "lint": "eslint --ext .ts,.vue src && stylelint **/*.vue",
    "update:emulator": "npx dotenv-cli -e .env.emulator -- flow project deploy --update",
    "update:testnet": "npx dotenv-cli -e .env.testnet -- flow project deploy --network=testnet -f flow.json -f flow.testnet.json --update",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@nuxtjs/tailwindcss": "^5.1.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "nuxt": "^3.0.0-rc.3",
    "postcss-custom-properties": "^12.1.7",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "rollup-plugin-polyfill-node": "^0.9.0",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.7.3",
    "vite-svg-loader": "^3.3.0"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.4",
    "@heroicons/vue": "^1.0.6",
    "@onflow/fcl": "latest",
    "@onflow/types": "^1.0.1",
    "@vueuse/core": "^8.6.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "cross-env": "^7.0.3",
    "daisyui": "^2.15.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.8",
    "json-rpc-engine": "^6.1.0",
    "vue-gtag": "^2.0.1",
    "vue3-lazyload": "^0.3.2",
    "web3modal-vue3": "^1.3.4"
  }
}
